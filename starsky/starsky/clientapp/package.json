{
  "name": "clientapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "abortcontroller-polyfill": "^1.3.0",
    "intersection-observer": "^0.7.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:ci": "react-scripts test --watchAll=false --coverage --reporters=default 2>&1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!coverage/**",
      "!node_modules/**",
      "!src/index.tsx",
      "!src/setupTests.js",
      "!public/**",
      "!build/**"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "json",
      "cobertura"
    ]
  },
  "proxy": "http://localhost:5000",
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/reach__router": "^1.2.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "typescript": "3.5.3"
  }
}
