using System.Collections.Generic;
using System.Diagnostics.Metrics;
using starsky.foundation.platform.ActivitySource;

namespace starsky.foundation.webtelemetry.Helpers;

public class SampleMeter
{
	public void Meter1()
	{
		using var meter = new Meter(ActivitySourceMeter.NameSpace, "1.0");
		var successCounter = meter.CreateCounter<long>("srv.successes.count",
			description: "Number of successful responses");
		successCounter.Add(1, new KeyValuePair<string, object?>("tagName", "tagValue"));
	}
}
