<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0</TargetFramework>
        <Nullable>enable</Nullable>
        <!-- SonarQube needs this -->
        <ProjectGuid>{a030c158-2f79-4317-a9f9-bdd46d66d1d8}</ProjectGuid>
        <RuntimeFrameworkVersion>6.0.21</RuntimeFrameworkVersion>
        <DebugType>Full</DebugType>
        <Version>0.5.10</Version>
    </PropertyGroup>
    
    <ItemGroup>
        <ProjectReference Include="..\starsky.feature.geolookup\starsky.feature.geolookup.csproj" />
        <ProjectReference Include="..\starsky.foundation.consoletelemetry\starsky.foundation.consoletelemetry.csproj" />
