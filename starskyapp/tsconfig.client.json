{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "noImplicitAny": true,
    "sourceMap": true,
    "outDir": "build",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    },
    "moduleResolution": "Node"
  },
  "include": [
    "src/client/**/*",
    "src/preload/**/*",
  ]
}
