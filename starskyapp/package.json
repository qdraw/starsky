{
  "name": "starsky",
  "version": "0.2.5",
  "description": "Starsky",
  "main": "index.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "dist": "electron-builder build",
    "mac": "electron-builder build -m",
    "windows": "electron-builder build -w",
    "macwindows": "electron-builder build -mw"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^8.2.3",
    "electron-builder": "^22.5.1"
  },
  "dependencies": {
    "extract-zip": "^2.0.0"
  },
  "build": {
    "extraResources": [
      "include-starsky-${os}.zip"
    ],
    "beforeBuild": "./scripts/beforeBuild.js",
    "appId": "nl.qdraw.starsky",
    "artifactName": "${productName}-${os}-desktop.${ext}",
    "mac": {
      "minimumSystemVersion": "10.12",
      "icon": "media/baseline-searcher-detective-263238__512px.icns",
      "target": [
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": [
        "zip"
      ],
      "icon": "media/baseline-searcher-detective-263238__256px.ico"
    }
  }
}
